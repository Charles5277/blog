    "Distro",
    "dpkg",
    "eamodio",
    "ecmel",
    "elit",
    "env",
    "eqeqeq",
    "errorlens",
    "evenodd",
    "featurename",
    "fetchpriority",
    "Firestore",
    "flexbox",
    "formulahendry",
    "frontmatter",
    "giscus",
    "gitmoji",
    "hardlinks",
    "Hexschool",
    "Homescreen",
    "iqjt",
    "Jamstack",
    "jsoncrack",
    "keyframes",
    "keyrings",
    "kohler",
    "lastval",
    "linebreak",
    "Linktree",
    "mediafeature",
    "mergetool",
    "mhutchie",
    "moby",
    "MRZLKR",
    "ncal",
    "nchc",
    "NOPASSWD",
    "norestart",
    "Noto",
    "npmjs",
    "NYUST",
    "oderwat",
    "owliehq",
    "Pagila",
    "Parens",
    "pflannery",
    "Pratice",
    "redjue",
    "reflog",
    "rgba",
    "Rolldown",
    "sdras",
    "sibiraj",
    "signin",
    "singleline",
    "sourcelist",
    "strikethrough",
    "Supabase",
    "swtich",
    "tabout",
    "tailwindcss",
    "todos",
    "TRUNC",
    "tseslint",
    "unalias",
    "unjs",
    "unstage",
    "usernamehw",
    "userprofile",
    "versionlens",
    "VITE",
    "vitepress",
    "vuefire",
    "vuestic",
    "wallabyjs",
    "webp",
    "whatis",
    "winget",
    "workdir",
    "worktree",
    "wslconfig",
    "xlink",
    "Zcfg",
    "Zeabur",
    "Zerotier",
    "zless"
  ],
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "versionlens.suggestions.showOnStartup": true,
  "eslint.validate": [
    "javascript",
    "typescript",
    "vue",
    "html",
    "markdown",
    "json"
  ],
  "eslint.rules.customizations": [
    {
      "rule": "style/*",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "format/*",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*-spacing",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*-spaces",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*-order",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*-dangle",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*-newline",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*quotes",
      "severity": "off",
      "fixable": true
    },
    {
      "rule": "*semi",
      "severity": "off",
      "fixable": true
    }
  ]
}
